#define LibStarch_MAJOR @LibStarch_VERSION_MAJOR@
#define LibStarch_MINOR @LibStarch_VERSION_MINOR@
#define LibStarch_WVERSION L"@LibStarch_VERSION@"
#define LibStarch_VERSION "@LibStarch_VERSION@"


#include <string>

namespace LibStarch {
  static std::wstring getWVersion() {
      return LibStarch_WVERSION;
  }

  static std::string getVersion() {
      return LibStarch_VERSION;
  }

#if defined(_WIN32) || defined(_WIN64)

  static std::wstring getAutoVersion() {
      return LibStarch_WVERSION;
  }

#else
  static std::string getAutoVersion() {
      return LibStarch_VERSION;
  }
#endif
}

